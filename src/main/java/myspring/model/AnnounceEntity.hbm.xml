<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="myspring.model.Announce" table="announce" schema="bulletin_board">
        <id name="announceid" column="announceid"/>
        <property name="announceTitle" column="announce_title"/>
        <property name="announceText" column="announce_text"/>
        <property name="announceFoto" column="announce_foto"/>
        <property name="announceAuthor" column="announce_author"/>
    </class>
</hibernate-mapping>